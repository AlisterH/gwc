	@$(NORMAL_INSTALL)
	@list='$(pixmap_DATA)'; test -n "$(pixmapdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(pixmapdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(pixmapdir)" || exit 1; \
	fi; \
	for p in $$list; do \
